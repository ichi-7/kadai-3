<%= render '/homes/info.html.erb', book_new: @book_new %>
<h2>Users</h2>
<table>
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= image_tag user.get_profile_image(100,100) %></td>
    <td><%= user.name %></td>
    <td><%= link_to "show", user_path(user) %></td>
  </tr>
  <% end %>
</table>